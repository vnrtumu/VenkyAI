{
    "productName": "VenkyAI",
    "version": "0.1.0",
    "identifier": "com.venkyai.app",
    "build": {
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist"
    },
    "app": {
        "macOSPrivateApi": true,
        "windows": [
            {
                "label": "overlay",
                "title": "VenkyAI",
                "transparent": true,
                "decorations": false,
                "alwaysOnTop": true,
                "skipTaskbar": true,
                "width": 400,
                "height": 650,
                "x": 50,
                "y": 80,
                "resizable": true,
                "shadow": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png"
        ]
    }
}